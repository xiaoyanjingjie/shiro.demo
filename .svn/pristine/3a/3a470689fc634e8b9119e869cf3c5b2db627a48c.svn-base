        id,
      </if>
      <if test="meetingtitle != null" >
        meetingtitle,
      </if>
      <if test="meetingtime != null" >
        meetingtime,
      </if>
       <if test="meetingremark != null" >
        meetingremark,
      </if>
      <if test="createtime != null" >
        createtime,
      </if>
      <if test="createtor != null" >
        createtor,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        #{id,jdbcType=BIGINT},
      </if>
      <if test="meetingtitle != null" >
